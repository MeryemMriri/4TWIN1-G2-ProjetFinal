
pipeline{
  agent any



  stages {

  stage('clean') {
              steps {
                    sh ('mvn clean')
              }
          }

  stage('Test') {
              steps {
                    sh ('mvn test')
              }
          }
  
  stage('compile') {
              steps {
                    sh ('mvn compile')
              }
          }
  }
  
node {
  stage('SCM') {
    checkout scm
  }
  stage('SonarQube Analysis') {
    def mvn = tool 'Default Maven';
    withSonarQubeEnv() {
      sh "${mvn}/bin/mvn clean verify sonar:sonar -Dsonar.projectKey=Youssef-GHARRAD-4TWIN1-G2 -Dsonar.projectName='Youssef-GHARRAD-4TWIN1-G2'"
    }
  }
}
}

