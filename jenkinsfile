
pipeline{
  agent any



  stages {

  stage('intall depencies') {
              steps {
                  script{
                    sh ('mvn install')
                  }
              }
          }

  stage('Unit Test') {
              steps {
                  script{
                    sh ('mvn test')
                  }
              }
          }
  
  stage('compile') {
              steps {
                  script{
                    sh ('mvn compile')
                  }
              }
          }


  }

}

